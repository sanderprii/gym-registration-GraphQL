{
  "name": "gym-registration-graphql-api",
  "version": "1.0.0",
  "description": "Gym Registration GraphQL API with functional equivalence to REST and SOAP",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "bash tests/test.sh",
    "test:client": "node client/example.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "validate:schema": "npx graphql-schema-linter schema/*.graphql"
  },
  "keywords": [
    "gym",
    "registration",
    "api",
    "graphql",
    "apollo",
    "nodejs",
    "express",
    "prisma"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@prisma/client": "^5.21.1",
    "apollo-server-express": "^3.12.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "axios": "^1.6.0",
    "graphql-schema-linter": "^3.0.1",
    "nodemon": "^3.0.3",
    "prisma": "^5.21.1"
  }
}
